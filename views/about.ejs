<%- include("header") -%>

<div class="container-fluid">

    <!-- Button back to home -->

    <div class="row column form-row responsive">
        <div class="col-lg-12">
            <form action="/backtohome" method="post">
                <button class="btn btn-sm btn-primary" type="submit">Back to home</button>
            </form>
        </div>
    </div>

    <!-- Slide Show -->

    <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
            <div class="carousel-inner about-box">
              <div class="carousel-item active">
                <img src="/img/developer.jpg "  alt="Ram Chhabra">
                <div class="about">
                    <h4>CEO</h4>
                    <h4>Ram Chhabra</h4>
                    <p>"Currently Study at NIT Hamirpur as a Computer Science student"</p>
                </div>
              </div>
              <div class="carousel-item">
                <img src="/img/roshan.jpg "  alt="Roshan Makar">
                <div class="about">
                    <h4>CO FOUNDER</h4>
                    <h4>Roshan Makar</h4>
                    <p>"Just pass-out 8th class with best knowledge"</p>
                </div>
              </div>
              <div class="carousel-item">
                <img src="/img/vikas.jpeg "  alt="Vikas Suthar">
                <div class="about">
                    <h4>VISITOR</h4>
                    <h4>Vikas Suthar</h4>
                    <p>"Study in Bhojrasar private school"</p>
                </div>
              </div>
            </div>
            <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
              <span class="carousel-control-prev-icon" aria-hidden="true"></span>
              <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
              <span class="carousel-control-next-icon" aria-hidden="true"></span>
              <span class="sr-only">Next</span>
            </a>
          </div>

</div>




